{
  "manifest_version": 2,

  "name": "Auranote Web Clipper",
  "description": "Save elements of web pages to Aura Note.",
  "version": "1.0",

  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],

  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },

  "background": {
    "scripts": ["clipper.js"]
  },

  "options_page": "options.html"
}