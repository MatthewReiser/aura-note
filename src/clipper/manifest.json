{
  "manifest_version": 2,

  "name": "Auranote Web Clipper",
  "description": "Save elements of web pages to Aura Note.",
  "version": "1.0",

  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],

  "background": {
    "scripts": ["clipper.js"]
  },

  "options_page": "options.html"
}