<lumen:component model="java://org.lumenframework.demo.notes.models.NoteListModel">
	<lumen:attribute name="sort" type="String" default="createdOn.desc"/>
	<lumen:attribute name="query" type="String"/>
	
	<lumen:foreach items="{!m.notes}" var="note">
       	<lumennote:noteListRow note="{!note}"/>
    </lumen:foreach>
</lumen:component>
