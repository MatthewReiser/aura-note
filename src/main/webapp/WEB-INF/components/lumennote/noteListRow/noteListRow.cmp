<lumen:component extends="lumennote:listRow">
    <lumen:attribute name="note" type="java://org.lumenframework.demo.notes.Note"/>
	<lumen:set attribute="right"><ui:outputDateTime value="{!v.note.createdOnAsCalendar}"/></lumen:set>
    <lumen:set attribute="desc">You see? It's curious. Ted did figure it out - time travel. And when we get back, we gonna tell everyone. How it's possible, how it's done, what the dangers are.</lumen:set>
    <a onclick="{!c.openNote}">{!v.note.title}</a>
</lumen:component>